{
  "manifest_version": 3,
  "name": "Summarize Webpage",
  "version": "1.0",
  "description": "Summarize Webpage Content",
  "permissions": ["tabs", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"]
}
